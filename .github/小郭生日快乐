local JumpInput = Tab:Input({
    Title = "Ë∑≥Ë∑ÉÈ´òÂ∫¶ËÆæÁΩÆ",
    Desc = "ËæìÂÖ•Êï∞Â≠ó‰øÆÊîπÁé©ÂÆ∂Ë∑≥Ë∑ÉÈ´òÂ∫¶",
    Value = "",
    InputIcon = "arrow-up-from-line", -- ÂõæÊ†áÔºàÂæÄ‰∏äË∑≥ÁöÑÊ†∑ÂºèÔºâ
    Type = "Input",
    Placeholder = "ËØ∑ËæìÂÖ•Ë∑≥Ë∑ÉÈ´òÂ∫¶Êï∞ÂÄº...",
    Callback = function(text)
        local jump = tonumber(text) -- ËΩ¨Êç¢‰∏∫Êï∞Â≠ó
        if jump then
            local humanoid = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.UseJumpPower = true -- Á°Æ‰øùÂêØÁî® JumpPower Ê®°Âºè
                humanoid.JumpPower = jump
                print("Â∑≤Â∞ÜÁé©ÂÆ∂Ë∑≥Ë∑ÉÈ´òÂ∫¶‰øÆÊîπ‰∏∫: " .. jump)
            else
                print("Êú™ÊâæÂà∞HumanoidÔºåÂèØËÉΩËßíËâ≤ËøòÊ≤°Âä†ËΩΩ")
            end
        else
            print("ËØ∑ËæìÂÖ•Ê≠£Á°ÆÁöÑÊï∞Â≠óÔºÅ")
        end
    end
})

