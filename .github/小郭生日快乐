local GravityInput = Tab:Input({
    Title = "ÈáçÂäõËÆæÁΩÆ",
    Desc = "ËæìÂÖ•Êï∞Â≠ó‰øÆÊîπÂÖ®Â±ÄÈáçÂäõ (ÈªòËÆ§196.2)",
    Value = "",
    InputIcon = "circle-dot", -- ÂõæÊ†áÈöèÊÑèÔºåËøôÈáåÁî®ÂúÜÁÇπ
    Type = "Input",
    Placeholder = "ËØ∑ËæìÂÖ•ÈáçÂäõÊï∞ÂÄº...",
    Callback = function(text)
        local gravity = tonumber(text) -- ËΩ¨Êç¢‰∏∫Êï∞Â≠ó
        if gravity then
            Workspace.Gravity = gravity
            print("Â∑≤Â∞ÜÈáçÂäõ‰øÆÊîπ‰∏∫: " .. gravity)
        else
            print("ËØ∑ËæìÂÖ•Ê≠£Á°ÆÁöÑÊï∞Â≠óÔºÅ")
        end
    end
})



