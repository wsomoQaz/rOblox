local SpeedInput = Tab:Input({
    Title = "ÁßªÂä®ÈÄüÂ∫¶ËÆæÁΩÆ",
    Desc = "ËæìÂÖ•Êï∞Â≠ó‰øÆÊîπÁé©ÂÆ∂ÈÄüÂ∫¶",
    Value = "",
    InputIcon = "zap", -- ÂõæÊ†áÂèØ‰ª•ÊîπÔºåÊØîÂ¶Ç zapÔºàÈó™ÁîµÔºâ
    Type = "Input",
    Placeholder = "ËØ∑ËæìÂÖ•ÈÄüÂ∫¶Êï∞ÂÄº...",
    Callback = function(text)
        local speed = tonumber(text) -- ËΩ¨Êç¢‰∏∫Êï∞Â≠ó
        if speed then
            LocalPlayer.Character.Humanoid.WalkSpeed = speed
            print("Â∑≤Â∞ÜÁßªÂä®ÈÄüÂ∫¶‰øÆÊîπ‰∏∫: " .. speed)
        else
            print("ËØ∑ËæìÂÖ•Ê≠£Á°ÆÁöÑÊï∞Â≠óÔºÅ")
        end
    end
})

