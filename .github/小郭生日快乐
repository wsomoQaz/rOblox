local Button = Tab:Button({
    Title = "È£ûË°åÊåâÈíÆ",
    Desc = "ÁÇπÂáª‰ºöÊúâÁîüÊó•Á•ùÁ¶è",
    Locked = false,
    Callback = function()
game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "Â∞èÈÉ≠ÁîüÊó•Âø´‰πê";
                Text ="Â∞èÈÉ≠ÁîüÊó•Âø´‰πê";
                Duration = 2.5;
            })
-- Gui to Lua
-- Version: 3.2

-- Instances:

local main = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
