    Locked = false,
    Callback = function()
game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "小郭生日快乐";
                Text ="小郭生日快乐";
                Duration = 2.5;
            })
-- Gui to Lua
-- Version: 3.2

-- Instances:

local main = Instance.new("ScreenGui")
