-- è·å–æœåŠ¡
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local Camera = workspace.CurrentCamera
local UserInputService = game:GetService("UserInputService")
local LocalPlayer = Players.LocalPlayer

--================ FOV åœ† =================--
local FOVCircle = Drawing.new("Circle")
FOVCircle.Visible = false
FOVCircle.Color = Color3.fromRGB(255,0,0) -- çº¢è‰²
FOVCircle.Thickness = 2
FOVCircle.NumSides = 100
FOVCircle.Radius = 150
FOVCircle.Filled = false

RunService.RenderStepped:Connect(function()
    if FOVCircle.Visible then
        local viewportSize = Camera.ViewportSize
        FOVCircle.Position = Vector2.new(viewportSize.X/2, viewportSize.Y/2)
    end
end)

--=============== FOVæ˜¾ç¤ºå¼€å…³ ===============
Tab:Toggle({
    Title = "æ˜¾ç¤ºFOVèŒƒå›´",
    Default = false,
    Callback = function(value)
        FOVCircle.Visible = value
        print(value and "âœ… FOVæ˜¾ç¤ºå·²å¼€å¯" or "âŒ FOVæ˜¾ç¤ºå·²å…³é—­")
    end
})

--=============== FOVåŠå¾„è¾“å…¥æ¡† ===============
Tab:Input({
    Title = "è®¾ç½®FOVåŠå¾„",
    Desc = "è¾“å…¥æ•°å€¼åå›è½¦ç¡®è®¤",
    Placeholder = "ä¾‹å¦‚: 150",
    Callback = function(value)
        local num = tonumber(value)
        if num then
            FOVCircle.Radius = num
            print("ğŸ¯ FOVåŠå¾„å·²è®¾ç½®ä¸º "..num.." ç±³")
        else
            warn("âš ï¸ è¯·è¾“å…¥æ­£ç¡®æ•°å­—")
        end
    end
})

--=============== é˜µå®¹åˆ¤æ–­ Toggle ===============
local TeamCheckActive = false
Tab:Toggle({
    Title = "é˜µå®¹åˆ¤æ–­",
    Desc = "å¼€å¯åæ£€æµ‹æ•Œæ–¹ç©å®¶è¡€é‡",
    Icon = "users",
    Default = false,
    Callback = function(state)
        TeamCheckActive = state
        if state then
            print("âœ… é˜µå®¹åˆ¤æ–­å·²å¼€å¯")
            spawn(function()
                while TeamCheckActive do
                    for _, player in pairs(Players:GetPlayers()) do
                        if player ~= LocalPlayer and player.Team ~= LocalPlayer.Team then
                            local char = player.Character
                            local hum = char and char:FindFirstChildWhichIsA("Humanoid")
                            if hum then
                                print("æ•Œæ–¹ç©å®¶: "..player.Name.." è¡€é‡: "..math.floor(hum.Health))
                            end
                        end
                    end
                    wait(1)
                end
            end)
        else
            print("âŒ é˜µå®¹åˆ¤æ–­å·²å…³é—­")
        end
    end
})

--=============== Aimbot Toggle ===============
local AimbotActive = false
Tab:Toggle({
    Title = "è‡ªç„å¼€å…³",
    Desc = "å¼€å¯åè‡ªåŠ¨é”å®šFOVèŒƒå›´å†…æ•Œäººå¤´éƒ¨",
    Icon = "target",
    Default = false,
    Callback = function(state)
        AimbotActive = state
        if state then
            print("âœ… è‡ªç„å·²å¼€å¯")
            spawn(function()
                while AimbotActive do
                    local closestDist = math.huge
                    local target = nil
                    for _, player in pairs(Players:GetPlayers()) do
                        if player ~= LocalPlayer and player.Team ~= LocalPlayer.Team then
                            local char = player.Character
                            local hum = char and char:FindFirstChildWhichIsA("Humanoid")
                            local head = char and char:FindFirstChild("Head")
                            if hum and head and hum.Health > 0 then
                                -- è®¡ç®—å±å¹•è·ç¦»
                                local pos, onScreen = Camera:WorldToViewportPoint(head.Position)
                                local dist = (Vector2.new(pos.X,pos.Y) - Vector2.new(Camera.ViewportSize.X/2, Camera.ViewportSize.Y/2)).Magnitude
                                if onScreen and dist <= FOVCircle.Radius then
                                    if dist < closestDist then
                                        closestDist = dist
                                        target = player
                                    end
                                end
                            end
                        end
                    end
                    if target and target.Character then
                        local head = target.Character:FindFirstChild("Head")
                        if head then
                            -- æ˜¾ç¤ºåå­—å’Œè·ç¦»
                            local distance = (LocalPlayer.Character.HumanoidRootPart.Position - head.Position).Magnitude
                            print("ç›®æ ‡: "..target.Name.." è·ç¦»: "..string.format("%.1f",distance).." ç±³")
                            
                            -- è‡ªåŠ¨ç„å‡†é€»è¾‘ï¼ˆç®€å•ç¤ºä¾‹ï¼Œæ—‹è½¬æ‘„åƒæœºï¼‰
                            local direction = (head.Position - Camera.CFrame.Position).Unit
                            local cf = CFrame.new(Camera.CFrame.Position, Camera.CFrame.Position + direction)
                            Camera.CFrame = cf
                        end
                    end
                    wait(0.03) -- æ§åˆ¶ç„å‡†åˆ·æ–°
                end
            end)
        else
            print("âŒ è‡ªç„å·²å…³é—­")
        end
    end
})